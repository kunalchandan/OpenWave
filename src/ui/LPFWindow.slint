
import { LineEdit } from "std-widgets.slint";
export component LPFWindow {
    width: 256px;
    height: 128px;
    Rectangle {
        width: parent.width;
        height: parent.height;
        Text {
            x: parent.width / 9;
            y: parent.height / 9;
            text: "Low Pass Filter";
            font-family: "Lato";
            font-weight: 100;
            font-italic: true;
            font-size: 15px;
        }

        Path {
            x: 0;
            y: 0;
            width: parent.width;
            height: parent.height;
            stroke: blueviolet;
